CREATE EXTERNAL TABLE transactionlog_consol(tranID string,origTranType string,basicTranType string,description string,startDate string,endDate string,employee string,controlNum string,lineNum string,whID string,fromLoc string,toLoc string,palletID string,itemNo string,lotNo string,qty double,netWeight double,grossWeight double,customer string)ROW FORMAT DELIMITEDFIELDS TERMINATED BY '\001'STORED AS PARQUETLOCATION '/staging/consolidated/transactionlog_consol'